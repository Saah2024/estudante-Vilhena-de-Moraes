let cor;
let posicaoHorizontal; // x
let posecaoVertical; // y

function setup() {
  
  createCanvas(600, 600);
background("white");
cor = color(random(0, 255), random(0, 255), random(0, 255));
posicaoHorizontal = 200;
posicaoVertical =200;
}
  
function draw () {
  
  fill(cor);
    circle(200, 200, 50);

 
}
